		"pattern": "((var/s|new/s|function|private/s|if/s|else/s|for/s|in/s|document|window|ready))"
	}, {
		"class": "operator",
		"pattern": "((?:[\\+\\-\\=\\!\\|\\:\\[\\]\\(\\)\\{\\}\\>\\<\\$]|&amp;|&lt;|&gt;){1,}|(?:[^\\*\\/]\\/(?!\\/|\\*)))"
	}, {
		"class": "comment",
		"pattern": "\\/\\/[^\\(\\n|\\r)]+|\\/\\*|\\*\\/"
	}
]
